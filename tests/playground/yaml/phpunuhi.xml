<phpunuhi
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../schema/dev-main.xsd"
>

    <translations>

        <set name="yaml">
            <format>
                <yaml sort="true"/>
            </format>
            <locales>
                <locale name="de">./de.yaml</locale>
                <locale name="en">./en.yaml</locale>
            </locales>
        </set>

    </translations>


    <coverage>
        <locale name="de">20</locale>
    </coverage>

</phpunuhi>
